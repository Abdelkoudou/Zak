{
  "name": "fmc-app",
  "version": "2.0",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start --clear",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "build:web": "node -e \"require('fs').copyFileSync('global.web.css', 'global.css')\" && npx expo export --platform web && node -e \"require('fs').copyFileSync('public/favicon.ico', 'dist/favicon.ico')\"",
    "preview:web": "npx serve dist",
    "lint": "eslint .",
    "create:padded-icon": "node ./scripts/create-padded-icon.js",
    "fix:icons": "node ./scripts/fix-icons.js",
    "prebuild:clean": "npx expo prebuild --clean",
    "build:android": "eas build --platform android",
    "build:ios": "eas build --platform ios"
  },
  "dependencies": {
    "@expo/metro-runtime": "~5.0.5",
    "@hookform/resolvers": "^3.3.2",
    "@react-native-async-storage/async-storage": "2.1.2",
    "@supabase/supabase-js": "^2.39.0",
    "expo": "~53.0.0",
    "expo-blur": "~14.1.0",
    "expo-build-properties": "~0.14.0",
    "expo-constants": "~17.1.0",
    "expo-device": "~7.1.0",
    "expo-file-system": "~18.1.0",
    "expo-linear-gradient": "~14.1.0",
    "expo-linking": "~7.1.0",
    "expo-navigation-bar": "~4.2.8",
    "expo-router": "~5.1.10",
    "expo-secure-store": "~14.2.0",
    "expo-splash-screen": "~0.30.0",
    "expo-status-bar": "~2.2.0",
    "nativewind": "^4.2.1",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-hook-form": "^7.49.0",
    "react-native": "0.79.6",
    "react-native-css-interop": "^0.2.1",
    "react-native-edge-to-edge": "1.6.0",
    "react-native-reanimated": "~3.17.0",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-svg": "15.11.2",
    "react-native-url-polyfill": "^2.0.0",
    "react-native-web": "~0.20.0",
    "tailwindcss": "^3.4.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/react": "~19.0.10",
    "babel-plugin-module-resolver": "^5.0.2",
    "eslint": "^8.56.0",
    "serve": "^14.2.5",
    "sharp": "^0.32.6",
    "typescript": "~5.8.3"
  },
  "private": true
}
