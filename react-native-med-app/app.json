{
  "expo": {
    "name": "FMC Study App",
    "slug": "fmc-study-app",
    "version": "1.1.0",
    "orientation": "portrait",
    "scheme": "fmcstudyapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "backgroundColor": "#3B82F6"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.fmcstudyapp.mobile"
    },
    "android": {
      "package": "com.fmcstudyapp.mobile",
      "versionCode": 2,
      "adaptiveIcon": {
        "backgroundColor": "#3B82F6"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "672f6566-b5f9-4629-a0ef-cec5968ef4e6"
      }
    }
  }
}
